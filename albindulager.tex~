\begin{alignment}[
    texts=edition[class="edition"];
    translation[class="translation"],
  ]
\begin{edition}
 \ekddiv{type=ed}
    \bigskip
    \centerline{\textrm{\small{[Siddhakuṇḍalinīyoga and Mantrayoga]}}}
    \bigskip
    \begin{prose}
%-----------------------   
% \om                                   \B
%idānīṃ rājayogasya bhedāḥ kathyante // \E
